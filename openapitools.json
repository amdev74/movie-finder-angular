{
  "$schema": "node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "spaces": 2,
  "generator-cli": {
    "version": "7.10.0",
    "generators": {
      "movie-api": {
        "generatorName": "typescript-angular",
        "inputSpec": "./openapi/movie-finder-api.json",
        "output": "./src/app/core/api",
        "additionalProperties": {
          "npmName": "@movie-finder/api-client",
          "npmVersion": "1.0.0",
          "snapshot": false,
          "ngVersion": "21.0.0",
          "providedInRoot": true,
          "stringEnums": true,
          "withInterfaces": true,
          "useSingleRequestParameter": false
        },
        "typeMappings": {},
        "reservedWordsMappings": {}
      }
    }
  }
}